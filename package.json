{
  "name": "shadcn-location-autocomplete",
  "private": true,
  "sideEffects": false,
  "description": "A provider-agnostic location autocomplete component built with Shadcn UI and Tailwind CSS.",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "shadcn build && vite build",
    "types:check": "fumadocs-mdx && tsc --noEmit",
    "postinstall": "fumadocs-mdx",
    "lint": "biome check",
    "format": "biome format --write",
    "registry:build": "shadcn build",
    "deploy": "npm run build && wrangler deploy",
    "preview": "npm run build && vite preview",
    "cf-typegen": "wrangler types"
  },
  "dependencies": {
    "@base-ui/react": "^1.0.0",
    "@cloudflare/vite-plugin": "^1.17.1",
    "@tanstack/react-router": "^1.144.0",
    "@tanstack/react-router-devtools": "^1.144.0",
    "@tanstack/react-start": "^1.145.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "fumadocs-core": "16.4.1",
    "fumadocs-mdx": "14.2.3",
    "fumadocs-ui": "16.4.1",
    "lucide-react": "^0.562.0",
    "motion": "^12.23.26",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "shadcn": "^3.6.2",
    "tailwind-merge": "^3.4.0",
    "use-debounce": "^10.0.6",
    "vite": "^7.2.7"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@tailwindcss/vite": "^4.1.18",
    "@types/mdx": "^2.0.13",
    "@types/node": "^24.10.2",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "nitro": "3.0.1-alpha.1",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^6.0.0",
    "wrangler": "^4.54.0"
  }
}
